{
  "name": "protochain-ts",
  "version": "1.0.0",
  "description": "Blockchain Prototype created with Typescript",
  "main": "index.js",
  "scripts": {
    "compile": "tsc",
    "dev": "npx nodemon --watch './src/' --exec 'ts-node ./src/lib/blockchain.ts' -e ts",
    "start": "node ./dist/blockchain.js",
    "test": "jest",
    "lint": "eslint src --ext .ts --fix",
    "blockchain": "npx ts-node ./src/server/blockchainServer.ts --run",
    "miner": "npx ts-node ./src/client/minerClient.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "morgan": "^1.10.0"
  }
}
